<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SalazarEExamenProgreso2.AboutPage"
             Title="AboutPage">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        
        <Label 
               Text="Eduardo"
               HorizontalOptions="Center"
               VerticalOptions="Center" />
        
        <Label Grid.Column="1"
               Text="Esteban"
               HorizontalOptions="Center"
               VerticalOptions="Center" />
        
        <Label Grid.Row="1"
               Text="Salazar"
               HorizontalOptions="Center"
               VerticalOptions="Center" />
        
        <Label Grid.Row="1"
               Grid.Column="1"
               Text="Castro"
               HorizontalOptions="Center"
               VerticalOptions="Center" />
    </Grid>
</ContentPage>